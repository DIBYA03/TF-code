{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PutObjects",
      "Effect": "Allow",
      "Action": [
        "kms:Encrypt",
        "kms:GenerateDataKey",
        "s3:AbortMultipartUpload",
        "s3:ListBucketMultipartUploads",
        "s3:ListMultipartUploadParts",
        "s3:PutObject*"
      ],
      "Resource": [
        "${kms_arn}",
        "arn:aws:s3:::${bucket_name}/*"
      ]
    }
  ]
}