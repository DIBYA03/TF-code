[
  {
    "name": "${name}",
    "image": "${ecr_image}:${ecr_image_tag}",
    "essential": true,
    "cpu": ${fargate_cpu
    },
    "memory": ${fargate_memory
    },
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/${aws_log_group}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${name}"
      }
    },
    "environment": [
      {
        "name": "API_ENV",
        "value": "${api_env}"
      },
      {
        "name": "CSP_REVIEW_SQS_REGION",
        "value": "${csp_review_sqs_region}"
      },
      {
        "name": "CSP_REVIEW_SQS_URL",
        "value": "${csp_review_sqs_url}"
      },
      {
        "name": "GRPC_SERVICE_PORT",
        "value": "${grpc_port}"
      },
      {
        "name": "USE_TRANSACTION_SERVICE",
        "value": "${use_transaction_service}"
      },
      {
        "name": "USE_BANKING_SERVICE",
        "value": "${use_banking_service}"
      },
      {
        "name": "KINESIS_TRX_NAME",
        "value": "${kinesis_trx_name}"
      },
      {
        "name": "KINESIS_TRX_REGION",
        "value": "${kinesis_trx_region}"
      },
      {
        "name": "SEGMENT_SQS_URL",
        "value": "${segment_sqs_url}"
      },
      {
        "name": "SQS_REGION",
        "value": "${sqs_region}"
      },
      {
        "name": "SQS_URL",
        "value": "${sqs_url}"
      },
      {
        "name": "AWS_S3_BUCKET_DOCUMENT",
        "value": "${aws_s3_bucket_document}"
      },
      {
        "name": "USE_INVOICE_SERVICE",
        "value": "${use_invoice_service}"
      }
    ],
    "secrets": [
      {
        "name": "BBVA_APP_ENV",
        "valueFrom": "${bbva_app_env}"
      },
      {
        "name": "BBVA_APP_ID",
        "valueFrom": "${bbva_app_id}"
      },
      {
        "name": "BBVA_APP_NAME",
        "valueFrom": "${bbva_app_name}"
      },
      {
        "name": "BBVA_APP_SECRET",
        "valueFrom": "${bbva_app_secret}"
      },
      {
        "name": "CORE_DB_WRITE_URL",
        "valueFrom": "${core_db_write_url}"
      },
      {
        "name": "CORE_DB_READ_URL",
        "valueFrom": "${core_db_read_url}"
      },
      {
        "name": "CORE_DB_WRITE_PORT",
        "valueFrom": "${core_db_write_port}"
      },
      {
        "name": "CORE_DB_READ_PORT",
        "valueFrom": "${core_db_read_port}"
      },
      {
        "name": "CORE_DB_NAME",
        "valueFrom": "${core_db_name}"
      },
      {
        "name": "CORE_DB_USER",
        "valueFrom": "${core_db_user}"
      },
      {
        "name": "CORE_DB_PASSWD",
        "valueFrom": "${core_db_passwd}"
      },
      {
        "name": "BANK_DB_WRITE_URL",
        "valueFrom": "${bank_db_write_url}"
      },
      {
        "name": "BANK_DB_READ_URL",
        "valueFrom": "${bank_db_read_url}"
      },
      {
        "name": "BANK_DB_WRITE_PORT",
        "valueFrom": "${bank_db_write_port}"
      },
      {
        "name": "BANK_DB_READ_PORT",
        "valueFrom": "${bank_db_read_port}"
      },
      {
        "name": "BANK_DB_NAME",
        "valueFrom": "${bank_db_name}"
      },
      {
        "name": "BANK_DB_USER",
        "valueFrom": "${bank_db_user}"
      },
      {
        "name": "BANK_DB_PASSWD",
        "valueFrom": "${bank_db_passwd}"
      },
      {
        "name": "IDENTITY_DB_WRITE_URL",
        "valueFrom": "${identity_db_write_url}"
      },
      {
        "name": "IDENTITY_DB_READ_URL",
        "valueFrom": "${identity_db_read_url}"
      },
      {
        "name": "IDENTITY_DB_WRITE_PORT",
        "valueFrom": "${identity_db_write_port}"
      },
      {
        "name": "IDENTITY_DB_READ_PORT",
        "valueFrom": "${identity_db_read_port}"
      },
      {
        "name": "IDENTITY_DB_NAME",
        "valueFrom": "${identity_db_name}"
      },
      {
        "name": "IDENTITY_DB_USER",
        "valueFrom": "${identity_db_user}"
      },
      {
        "name": "IDENTITY_DB_PASSWD",
        "valueFrom": "${identity_db_passwd}"
      },
      {
        "name": "TXN_DB_WRITE_URL",
        "valueFrom": "${txn_db_write_url}"
      },
      {
        "name": "TXN_DB_READ_URL",
        "valueFrom": "${txn_db_read_url}"
      },
      {
        "name": "TXN_DB_WRITE_PORT",
        "valueFrom": "${txn_db_write_port}"
      },
      {
        "name": "TXN_DB_READ_PORT",
        "valueFrom": "${txn_db_read_port}"
      },
      {
        "name": "TXN_DB_NAME",
        "valueFrom": "${txn_db_name}"
      },
      {
        "name": "TXN_DB_USER",
        "valueFrom": "${txn_db_user}"
      },
      {
        "name": "TXN_DB_PASSWD",
        "valueFrom": "${txn_db_passwd}"
      },
      {
        "name": "FIREBASE_CONFIG",
        "valueFrom": "${firebase_config}"
      },
      {
        "name": "SENDGRID_API_KEY",
        "valueFrom": "${sendgrid_api_key}"
      },
      {
        "name": "WISE_CLEARING_ACCOUNT_ID",
        "valueFrom": "${wise_clearing_account_id}"
      },
      {
        "name": "WISE_CLEARING_BUSINESS_ID",
        "valueFrom": "${wise_clearing_business_id}"
      },
      {
        "name": "WISE_CLEARING_USER_ID",
        "valueFrom": "${wise_clearing_user_id}"
      },
      {
        "name": "WISE_SUPPORT_EMAIL",
        "valueFrom": "${wise_support_email}"
      },
      {
        "name": "WISE_SUPPORT_NAME",
        "valueFrom": "${wise_support_name}"
      },
      {
        "name": "WISE_INVOICE_EMAIL",
        "valueFrom": "${wise_invoice_email}"
      },
      {
        "name": "TWILIO_ACCOUNT_SID",
        "valueFrom": "${twilio_account_sid}"
      },
      {
        "name": "TWILIO_API_SID",
        "valueFrom": "${twilio_api_sid}"
      },
      {
        "name": "TWILIO_API_SECRET",
        "valueFrom": "${twilio_api_secret}"
      },
      {
        "name": "TWILIO_SENDER_PHONE",
        "valueFrom": "${twilio_sender_phone}"
      }
    ]
  }
]
