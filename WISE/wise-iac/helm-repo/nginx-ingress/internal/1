apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: nginx
    nginx.ingress.kubernetes.io/whitelist-source-range: 0.0.0.0/0
  name: demo-app
spec:
  rules:
    - host: k8s.sbx.wise.us
      http:
        paths:
          - backend:
              serviceName: demo-service
              servicePort: 8080
            path: /
